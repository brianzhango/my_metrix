{
  "name": "my_metrix",
  "version": "1.0.0",
  "description": "MERN stack",
  "main": "server.js",
  "proxy": "http://localhost:8082",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon server.js",
    "build": "cd front-end && npm run build",
    "install-client": "cd front-end && npm install",
    "client": "npm start --prefix front-end",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianzhango/my_metrix.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "brian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brianzhango/my_metrix/issues"
  },
  "homepage": "https://github.com/brianzhango/my_metrix#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.10.0",
    "mongoose": "^6.6.1",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2",
    "react": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-toastify": "^9.0.8",
    "svg-url-loader": "^8.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1"
  }
}
